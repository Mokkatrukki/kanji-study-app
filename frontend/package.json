{
    "name": "frontend",
    "version": "1.0.0",
    "main": "dist/script.js",
    "scripts": {
        "build:css": "./node_modules/.bin/tailwindcss -i ./src/input.css -o ./dist/output.css",
        "build:ts": "tsc",
        "build": "npm run build:css && npm run build:ts",
        "watch:css": "./node_modules/.bin/tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
        "watch:ts": "tsc --watch",
        "dev": "npm run build && concurrently \"npm:watch:*\"",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "concurrently": "^9.1.2",
        "tailwindcss": "^3.3.0",
        "typescript": "^5.8.3"
    }
}
